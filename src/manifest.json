{
  "name": "webOTP",
  "description": "Web-based Encrypted One Time Password Generator",
  "author": "faizath",
  "version": "0.1.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/faizath/webOTP",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "64": "icons/64x64.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png",
      "48": "icons/48x48.png",
      "64": "icons/64x64.png"
    },
    "default_title": "webOTP",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
